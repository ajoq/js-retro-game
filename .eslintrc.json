{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
          "error",
          "LabeledStatement",
          "WithStatement"
        ],
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "guard-for-in" : 0,
        "no-alert": "off"
      }
}
